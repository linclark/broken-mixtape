---
  globals:
    describe: true
    expect: true
    it: true
    jest: true
  env:
    node: true
    browser: true
  ecmaFeatures:
    jsx: true
  plugins:
    - react
  rules:
    "brace-style":
      - 2
      - "1tbs"
      - allowSingleLine: true
    "camelcase": 2
    "comma-spacing":
      - 2
      -
        "before": false
        "after": true
    "comma-style":
      - 2
      - "last"
    "complexity": 1
    "curly": 2
    "dot-location":
      - 1
      - "property"
    "dot-notation": 2
    "eol-last": 2
    "generator-star-spacing":
      - 1
      - "after"
    "indent":
      - 2
      - 2
      -
        SwitchCase: 1
    "key-spacing":
      - 1
      -
        "beforeColon": false
        "afterColon": true
    "max-len":
      - 1
      - 80
    "new-cap":
      - 2
      -
        "capIsNew": false
    "no-caller": 2
    "no-cond-assign": 2
    "no-debugger": 2
    "no-delete-var": 2
    "no-dupe-args": 2
    "no-dupe-keys": 2
    "no-else-return": 2
    "no-empty": 2
    "no-eval": 2
    "no-ex-assign": 2
    "no-extend-native": 2
    "no-extra-bind": 2
    "no-extra-semi": 2
    "no-fallthrough": 2
    "no-lonely-if": 2
    "no-mixed-spaces-and-tabs": 2
    "no-multi-str": 1
    "no-multiple-empty-lines":
      - 1
      -
        "max": 1
    "no-native-reassign": 2
    "no-nested-ternary": 2
    "no-new-require": 0
    "no-path-concat": 0
    "no-plusplus": 0
    "no-proto": 2
    "no-redeclare": 2
    "no-reserved-keys": 0
    "no-self-compare": 2
    "no-shadow": 1
    "no-shadow-restricted-names": 2
    "no-spaced-func": 1
    "no-ternary": 0
    "no-throw-literal": 2
    "no-trailing-spaces": 2
    "no-undef": 2
    "no-underscore-dangle": 0
    "no-unreachable": 2
    "no-unused-vars": 2
    "no-warning-comments": 0
    "no-with": 2
    "one-var": 0
    "padded-blocks":
      - 1
      - "never"
    "quotes":
      - 1
      - "double"
      - "avoid-escape"
    "radix": 2
    "semi":
      - 1
      - "always"
    "semi-spacing":
      - 1
      -
        "before": false
        "after": true
    "space-after-keywords":
      - 1
      - "always"
    "space-after-function-name": 0
    "space-before-blocks":
      - 1
      - "always"
    "space-before-function-paren":
      - 1
      - "never"
    "space-in-parens":
      - 1
      - "never"
    "space-return-throw-case": 1
    "spaced-comment":
      - 1
      - "always"
    "strict":
      - 2
      - "global"
    "use-isnan": 2
    "valid-typeof": 2
    "vars-on-top": 0
    "wrap-iife": 0
    "wrap-regex": 0
    "yoda": 2
